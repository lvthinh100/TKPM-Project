<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Sender</th>
      <th scope="col">Receiver</th>
      <th scope="col">Amount</th>
      <th scope="col">Time</th>
    </tr>
  </thead>
  <tbody>
    {{#each transactions}}
      <tr>
        <td>
          <div>
            {{id}}
          </div>
        </td>
        <td>{{sender}}</td>
        <td>{{receiver}}</td>
        <td>{{amount}}</td>
        <td>
          {{trans_time}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>